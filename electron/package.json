{
  "name": "longq-electron",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:frontend": "npm run build --prefix ../frontend",
    "dist:mac": "npm run build:frontend && electron-builder --mac",
    "dist:win": "npm run build:frontend && electron-builder --win"
  },
  "dependencies": {
    "mime": "^3.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.longq.operator",
    "productName": "Quantum Qiâ„¢ Operator",
    "files": [
      "**/*",
      "../backend/**/*",
      "../frontend/dist/**/*"
    ],
    "extraResources": [
      {
        "from": "../backend",
        "to": "backend"
      },
      {
        "from": "../frontend/dist",
        "to": "frontend/dist"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
