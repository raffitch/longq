{
  "name": "longq-electron",
  "private": true,
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:frontend": "npm run build --prefix ../frontend",
    "dist:mac": "npm run build:frontend && electron-builder --mac",
    "dist:win": "npm run build:frontend && electron-builder --win",
  "licenses": "node ../scripts/run_generate_all_licenses.cjs --group-by-license",
    "predist:mac": "npm run licenses",
    "predist:win": "npm run licenses"
  },
  "dependencies": {
    "mime": "^3.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.longq.operator",
    "productName": "Quantum Qiâ„¢ Operator",
    "files": [
      "main.js",
      "preload.js",
      "serve.js",
      "build/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "../backend",
        "to": "backend"
      },
      {
        "from": "../backend/runtime",
        "to": "backend-python"
      },
      {
        "from": "../frontend/dist",
        "to": "frontend/dist"
      },
      {
        "from": "../licenses",
        "to": "licenses"
      },
      {
        "from": "../frontend/public/quantum-qi-logo.png",
        "to": "assets/quantum-qi-logo.png"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icons/icon.ico"
    }
  }
}
